<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>INVOICE REPORT</ReportTitle><ReportDescription>The Invoice Report provides a list of the Purchase Orders placed in our store. </ReportDescription><PageSize>A4</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>yes</gridSearchable><gridPageable>yes</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT ven.vendor_name, po.purchase_order_code, po.po_date, 
CASE
  WHEN po.status = '1' THEN 'Open'
  WHEN  po.status = '2' THEN 'Partially Invoiced'
  WHEN  po.status = '3' THEN 'Fully Invoiced'
  WHEN  po.status = '4' THEN 'Rejected'
  WHEN  po.status = '5' THEN 'Cancelled'
  WHEN  po.status = '6' THEN 'Closed'
  ELSE '--'
END po_status, inv.inv_no, inv.inv_date, inv.permit_no, inv.bol_no, inv.vessel_name, fam.pro_family_name, prod.pro_name, vty.variety_name,
size.size_name, gde.grade_long_name, itm.inv_det_cotton_qty, itm.inv_det_price, itm.inv_det_ctnr_no, itm.inv_det_net_weight,
(itm.inv_det_cotton_qty * itm.inv_det_price) amount
FROM tbl_invoice_items itm
JOIN tbl_invoice inv
ON inv.invoice_id = itm.inv_id
JOIN tbl_vendor ven
ON ven.vendor_id = inv.vendor_id
JOIN tbl_purchase_order po
ON po.po_id = inv.po_id
JOIN tbl_product_family fam
ON fam.pro_family_id = itm.inv_det_prod_fmly_id
JOIN tbl_product prod
ON prod.product_id = itm.inv_det_product_id
JOIN tbl_product_variety vty
ON vty.variety_id = itm.inv_det_variety_id
LEFT OUTER JOIN tbl_product_size size
ON size.size_id = itm.inv_det_size
LEFT OUTER JOIN tbl_product_grade gde
ON gde.grade_id = itm.inv_det_grade</SQLRaw></SQL><QueryColumns><QueryColumn><Name>vendor_name</Name><TableName>ven</TableName><ColumnName>vendor_name</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Vendor Name</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>purchase_order_code</Name><TableName>po</TableName><ColumnName>purchase_order_code</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PO Number</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>po_date</Name><TableName>po</TableName><ColumnName>po_date</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PO Date</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>po_status</Name><TableName>