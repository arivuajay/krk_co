<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>PAYMENT REPORT</ReportTitle><ReportDescription>The Payment Report provides a list of the Purchase Orders placed in our store.</ReportDescription><PageSize>A4</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>yes</gridSearchable><gridPageable>yes</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT ven.vendor_name, po.purchase_order_code, inv.inv_no, pay.pay_type, pay.invoice_amount, pay.pay_inr_rate,
pay.pay_deal_id, pay.pay_date, pay.pay_amount, pay.pay_inr_amount, pay.pay_mode, pay.pay_ref_info, pay.pay_bank_name, pay.pay_transaction_id,
CASE
  WHEN pay.pay_shift_advise IS NOT NULL  THEN pay.pay_shift_advise
  WHEN pay.pay_shift_advise IS NULL  THEN '-'
  ELSE '--'
END shift_advise,
CASE
  WHEN pay.pay_debit_advise IS NOT NULL  THEN pay.pay_debit_advise
  WHEN pay.pay_debit_advise IS NULL  THEN '-'
  ELSE '--'
END debit_advise,
CASE
  WHEN pay.pay_deal_id_copy IS NOT NULL  THEN pay.pay_deal_id_copy
  WHEN pay.pay_deal_id_copy IS NULL  THEN '-'
  ELSE '--'
END deal_id_copy
FROM tbl_payment pay
JOIN tbl_vendor ven
ON ven.vendor_id = pay.vendor_id
JOIN tbl_purchase_order po
ON po.po_id = pay.po_id
JOIN tbl_invoice inv
ON inv.invoice_id = pay.invoice_id</SQLRaw></SQL><QueryColumns><QueryColumn><Name>vendor_name</Name><TableName>ven</TableName><ColumnName>vendor_name</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Vendor Name</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>purchase_order_code</Name><TableName>po</TableName><ColumnName>purchase_order_code</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>PO Number</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>inv_no</Name><TableName>inv</TableName><ColumnName>inv_no</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Invoice Number</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>pay_type</Name><TableN